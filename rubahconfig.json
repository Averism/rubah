{
    "jobs": [
        {
            "name": "package",
            "command": "load_json",
            "location": "package.json"
        },
        {
            "name": "index",
            "command": "line_read",
            "location": "src/index.ts"
        },
        {
            "name": "maincommands",
            "command": "map_to_file_content",
            "input": "index_maincommand",
            "location": "docs/commands/$x.md"
        },
        {
            "name": "readme",
            "command": "line_write",
            "location": "readme.md",
            "commentStyle": "html"
        }
    ],

    "mappingfile": "rubahmapping.json",
    "helpers": ["src/helpers/common","src/helpers/markdown","src/helpers/textparser"],
    "commands": ["src/commands/load_json","src/commands/line_write","src/commands/line_read"]
}